# AI Agents - Environment Variables
# Copy this file to .env and fill in your values

# Google API Configuration (Required for Backend)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Database Configuration (Automatically set by docker-compose)
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_USER=dev
POSTGRES_PASSWORD=dev
POSTGRES_DATABASE=chat_db

# MCP Server Configuration (Automatically set by docker-compose)
MCP_SERVER_HOST=mcp-server
MCP_SERVER_PORT=8080

# API Configuration (Automatically set by docker-compose)
API_BASE_URL=http://backend:8000
